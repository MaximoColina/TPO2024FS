{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/register",
        "dest": "/api/register"
      },
      {
        "src": "/api/login",
        "dest": "/api/login"
      },
      {
        "src": "/api/productos",
        "dest": "/api/productos"
      },
      {
        "src": "/api/productos/:id",
        "dest": "/api/productos/:id"
      },
      {
        "src": "/api/productos/delete/:id",
        "dest": "/api/productos/delete/:id"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  